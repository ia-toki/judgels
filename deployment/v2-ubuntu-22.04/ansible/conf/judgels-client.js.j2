  name: '{{ app_name }}',
  slogan: '{{ app_slogan }}',
  apiUrl: 'https://{{ nginx_domain_judgels_server_api }}/v2',
  welcomeBanner: {
    title: '{{ app_title }}',
    description: '{{ app_description }}',
  },
